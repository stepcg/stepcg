@font-face {
  font-family: 'Font Awesome 5 Free';
  src: url(<%= portal_asset_data_uri 'fonts/fontawesome-webfont.eot' %>);
  src: url(<%= portal_asset_data_uri 'fonts/fontawesome-webfont.eot' %>?#iefix) format("embedded-opentype"), url(<%= portal_asset_data_uri 'fonts/fontawesome-webfont.woff2' %>) format('woff2'), url(<%= portal_asset_data_uri 'fonts/fontawesome-webfont.woff' %>) format("woff"), url(<%= portal_asset_data_uri 'fonts/fontawesome-webfont.ttf' %>) format("truetype");
  font-weight: normal;
  font-style: normal;
}


.dataLoading {
  background-image: url(<%= portal_asset_data_uri 'gears.gif' %>);
}

.loading_icon {
  background-image: url(<%= portal_asset_data_uri 'loading_icon.gif' %>);
}
